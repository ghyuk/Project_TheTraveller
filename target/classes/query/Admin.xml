<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.project.main.admin.dao.AdminDao">
	<select id="contactList" parameterType="cvo" resultType="cvo">
		SELECT CONTACTNUM,
						CONTACTDATE,
						CONTACTNAME,
						CONTACTEMAIL,
						CONTACTCONTENT,
						CONTACTREPLY,
						C_STATUS
		FROM	CONTACT
	</select>

</mapper>
<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.project.main.admin.dao.AdminDao">
	<select id="contactList" parameterType="cvo" resultType="cvo">
		SELECT CONTACTNUM,
						TO_CHAR(CONTACTDATE, 'YYYY-MM-DD') AS CONTACTDATE,
						CONTACTNAME,
						CONTACTEMAIL,
						CONTACTCONTENT,
						CONTACTREPLY,
						C_STATUS
		FROM	CONTACT
	</select>

</mapper>